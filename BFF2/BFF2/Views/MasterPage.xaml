                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group7child6pic1.png"/>
                                <local:CustomImage Path="group7child6pic2.png"/>
                                <local:CustomImage Path="group7child6pic3.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Breastfeeding Premature Babies" TargetType="{x:Type local:Views.Group7Child7}" UqName="group7child7"/>
                    <local:MasterPageItem Title="Breastfeeding and Medication" TargetType="{x:Type local:Views.Group7Child8}" UqName="group7child8"/>
                    <local:MasterPageItem Title="Breastfeeding in Public" TargetType="{x:Type local:Views.PureImage}" UqName="group7child9">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group7child9pic1.png"/>
                                <local:CustomImage Path="group7child9pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Smoking, Drinking, and Drug Use While Breastfeeding" TargetType="{x:Type local:Views.Group8Child0}" UqName="group8child0"/>
                    <local:MasterPageItem Title="Breastfeeding on Your Period" TargetType="{x:Type local:Views.Group8Child1}" UqName="group8child1"/>
                    <local:MasterPageItem Title="Breastfeeding and Birth Control" TargetType="{x:Type local:Views.Group8Child2}" UqName="group8child2"/>
                    <local:MasterPageItem Title="Baby Growth Spurts" TargetType="{x:Type local:Views.Group8Child3}" UqName="group8child3"/>
                    <local:MasterPageItem Title="Breastfeeding When Sick" TargetType="{x:Type local:Views.PureImage}" UqName="group8child4">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group8child4pic1.png"/>
                                <local:CustomImage Path="group8child4pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Getting a Pump" TargetType="{x:Type local:Views.PureImage}" UqName="group9child0">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group9child0pic1.png"/>
                                <local:CustomImage Path="group9child0pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Storing and Thawing Pumped Milk" TargetType="{x:Type local:Views.PureImage}" UqName="group9child2">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group9child2pic1.png"/>
                                <local:CustomImage Path="group9child2pic2.png"/>
                                <local:CustomImage Path="group9child2pic3.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Bottle Feeding a Breastfed Baby (Paced Feedings)" TargetType="{x:Type local:Views.Group11Child4}" UqName="group11child4"/>
                </x:Array>
            </ListView.ItemsSource>
            <ListView.ItemTemplate>
                <DataTemplate>
                    <ViewCell>
                        <Grid Padding="5,10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Label Grid.Column="1" Text="{Binding Title}" />
                        </Grid>
                    </ViewCell>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>
