                                <local:CustomImage Path="group6child2pic4.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Sleep Regressions" TargetType="{x:Type local:Views.PureImage}" UqName="group6child3">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group6child3pic1.png"/>
                                <local:CustomImage Path="group6child3pic2.png"/>
                                <local:CustomImage Path="group6child3pic3.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Painful Latch" TargetType="{x:Type local:Views.Group7Child0}" UqName="group7child0"/>
                    <local:MasterPageItem Title="Sore Nipples" TargetType="{x:Type local:Views.Group7Child1}" UqName="group7child1"/>
                    <local:MasterPageItem Title="Flat or Inverted Nipples" TargetType="{x:Type local:Views.PureImage}" UqName="group7child2">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group7child2pic1.png"/>
                                <local:CustomImage Path="group7child2pic2.png"/>
                                <local:CustomImage Path="group7child2pic3.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Low Milk Supply" TargetType="{x:Type local:Views.Group7Child3}" UqName="group7child3"/>
                    <local:MasterPageItem Title="Engorgement" TargetType="{x:Type local:Views.Group7Child4}" UqName="group7child4"/>
                    <local:MasterPageItem Title="Clogged Ducts" TargetType="{x:Type local:Views.PureImage}" UqName="group7child5">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group7child5pic1.png"/>
                                <local:CustomImage Path="group7child5pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Breastfeeding After a C-Section" TargetType="{x:Type local:Views.PureImage}" UqName="group7child6">
