                    <local:MasterPageItem Title="Time-Saving Benefits" TargetType="{x:Type local:Views.PureImage}" UqName="group2child3">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group2child3pic1.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Most Women Don't Breastfeed" TargetType="{x:Type local:Views.PureImage}" UqName="group3child0">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group3child0pic1.png"/>
                                <local:CustomImage Path="group3child0pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Formula is More Nutritious" TargetType="{x:Type local:Views.PureImage}" UqName="group3child1">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group3child1pic1.png"/>
                                <local:CustomImage Path="group3child1pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Giving Formula and Breastmilk Together" TargetType="{x:Type local:Views.PureImage}" UqName="group3child2">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group3child2pic1.png"/>
                                <local:CustomImage Path="group3child2pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Breastfeeding Makes Breasts Sag" TargetType="{x:Type local:Views.PureImage}" UqName="group3child3">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group3child3pic1.png"/>
                                <local:CustomImage Path="group3child3pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
