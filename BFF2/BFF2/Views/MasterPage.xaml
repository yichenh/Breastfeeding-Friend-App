<?xml version="1.0" encoding="UTF-8"?>
<ContentPage 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    xmlns:local="using:BFF2"
    x:Class="BFF2.Views.MasterPage"
    Title="Table of Contents">
    <!-- 
        This page is a list of all the links in the master page. For views
        that only contain images, you can define the link's TargetType as
        {x:Type local:Views.PureImage} and pass in a list of paths to images.
    
        For more customized views, simply pass in the name of the view (as
        is done with the WelcomePage for example.
    -->
    <StackLayout>
        <ListView x:Name="listView" x:FieldModifier="public">
            <ListView.ItemsSource>
                <x:Array Type="{x:Type local:MasterPageItem}">
                    <local:MasterPageItem Title="Welcome" TargetType="{x:Type local:Views.WelcomePage}"/>
                    <local:MasterPageItem Title="Points To Remember" TargetType="{x:Type local:Views.PureImage}" UqName="group0child0">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group0child0pic1.png"/>
                                <local:CustomImage Path="group0child0pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="How the App Works" TargetType="{x:Type local:Views.PureImage}" UqName="group1child0">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group1child0pic1.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Benefits for Baby" TargetType="{x:Type local:Views.PureImage}" UqName="group2child0">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group2child0pic1.png"/>
                                <local:CustomImage Path="group2child0pic2.png"/>
                                <local:CustomImage Path="group2child0pic3.png"/>
                                <local:CustomImage Path="group2child0pic4.png"/>
                                <local:CustomImage Path="group2child0pic5.png"/>
                                <local:CustomImage Path="group2child0pic6.png"/>
                                <local:CustomImage Path="group2child0pic7.png"/>
                                <local:CustomImage Path="group2child0pic8.png"/>
                                <local:CustomImage Path="group2child0pic9.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Benefits for Mom" TargetType="{x:Type local:Views.PureImage}" UqName="group2child1">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group2child1pic1.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="Financial Benefits" TargetType="{x:Type local:Views.PureImage}" UqName="group2child2">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group2child2pic1.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
