                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group3child5pic1.png"/>
                                <local:CustomImage Path="group3child5pic2.png"/>
                            </x:Array>
                        </local:MasterPageItem.Images>
                    </local:MasterPageItem>
                    <local:MasterPageItem Title="BreastFeeding is Painful" TargetType="{x:Type local:Views.Group3Child6}" UqName="group3child6"/>
                    <local:MasterPageItem Title="You Don't Need Birth Control While Breastfeeding" TargetType="{x:Type local:Views.Group3Child7}" UqName="group3child7"/>
                    <local:MasterPageItem Title="Manual Expression" TargetType="{x:Type local:Views.Group4Child0}" UqName = "group4child0"/>
                    <local:MasterPageItem Title="Position that is Comfortable for both Mother and Baby" TargetType="{x:Type local:Views.Group4Child1}" UqName="group4child1"/>
                    <local:MasterPageItem Title="The Latch" TargetType="{x:Type local:Views.Group4Child2}" UqName="group4child2"/>
                    <local:MasterPageItem Title="Making Enough Milk" TargetType="{x:Type local:Views.Group5Child0}" UqName="group5child0"/>
                    <local:MasterPageItem Title="How To Know Baby is Getting Enough Milk" TargetType="{x:Type local:Views.Group5Child1}" UqName="group5child1"/>
                    <local:MasterPageItem Title="Survival Tips" TargetType="{x:Type local:Views.Group5Child2}" UqName="group5child2"/>
                    <local:MasterPageItem Title="Getting Support" TargetType="{x:Type local:Views.PureImage}" UqName="group5child3">
                        <local:MasterPageItem.Images>
                            <x:Array Type="{x:Type local:CustomImage}">
                                <local:CustomImage Path="group5child3pic1.png"/>
                                <local:CustomImage Path="group5child3pic2.png"/>
